@model  List<Car.Remnants>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
    $(function () {
        $("[data-autocomplete-source]").each(function () {
            var target = $(this);
            target.autocomplete({ source: target.attr("data-autocomplete-source") });
        });
    });
</script>

<div class="bg-box">
    <div class="container">

      

        <ul class="tabs">
            <li>
                <input type="radio" name="tabs" id="tab-1" checked>
                <label for="tab-1">
                    Быстрый поиск
                </label>
                <div class="tab-content">

                 
                    @using (Html.BeginForm("Search", "Home", FormMethod.Get))
                    {
                        <div class="search-box1">
                            @{
                                string searchstr = Request.Params["Part"];

                                @(ViewContext.RouteData.Values["Part"])



                                Uri myUri = new Uri(Request.Url.AbsoluteUri);
                                string param1 = HttpUtility.ParseQueryString(myUri.Query).Get("Part");





                                if (param1 == null)
                                {
                                    param1 = " ";
                                }

                                if (param1 == "")
                                {
                                    param1 = " ";
                                }
                            }
                            @Html.TextBox("Part", null, new { data_autocomplete_source = @Url.Action("AutocompleteSearch", "Home") })
                            <a href="#" class="btn"><input type="submit" name="SearchStore" value="Поиск" /></a>
                        </div>
                     }
                </div>
            </li>
            <li>
                <input type="radio" name="tabs" id="tab-2">

                <label for="tab-2">

                    VIN запрос

                </label>
                <div class="tab-content">
                   @using (Html.BeginForm("Index", "Home", FormMethod.Get))
                    {
                       <div class="search-box1">
                            @Html.TextBox("Vin")
                            @Html.DropDownList("SelectedAuto", new SelectList(ViewBag.UniqueAuto, "Value", "Text", "ViewBag.Auto"), "Выберите модель по параметрам")
                            <a href="#" class="btn" style="color:white"><input type="submit" name="SearchVin" value="Поиск" /></a>
                        </div>
                    }
                 </div>
            </li>

        </ul>
       

    </div>
</div>



