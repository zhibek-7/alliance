
@{
    ViewBag.Title = "Catalog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<script type="text/javascript">
    $(function () {
        $("[data-autocomplete-source]").each(function () {
            var target = $(this);
            target.autocomplete({ source: target.attr("data-autocomplete-source") });
        });
    });
</script>

<div class="bg-box">
    <div class="container">

        

        <ul class="tabs">

            <li>

                <input type="radio" name="tabs" id="tab-1" checked>

                <label for="tab-1">
                    Быстрый поиск
                </label>
                <div class="tab-content">
                    @using (Html.BeginForm("Search", "Home", FormMethod.Get))
                    {
                        <div class="search-box1">

                            @Html.TextBox("Part", null, new { data_autocomplete_source = @Url.Action("AutocompleteSearch", "Home") })
                            <a href="#" class="btn"><input type="submit" name="SearchStore" value="Поиск" /></a>
                        </div>
                    }
                </div>
            </li>
            <li>
                <input type="radio" name="tabs" id="tab-2">

                <label for="tab-2">

                    VIN запрос

                </label>
                <div class="tab-content">
                    @using (Html.BeginForm("Index", "Home", FormMethod.Get))
                    {
                        <div class="search-box1">
                            @Html.TextBox("Vin")
                            @Html.DropDownList("SelectedAuto", new SelectList(ViewBag.UniqueAuto, "Value", "Text", "ViewBag.Auto"), "Выберите модель по параметрам")
                            <a href="#" class="btn" style="color:white"><input type="submit" name="SearchVin" value="Поиск" /></a>
                        </div>
                    }
                </div>
            </li>

        </ul>
 

    </div>
</div>

<section class="content-wrapper main-content clear-fix">


    <div class="rounded">


       
        <div class="rounded">
            <h2>Каталог оригинальных запчастей</h2>
            <table>
                <tbody>
                    <tr>
                        <td style="width: 50%">
                            <p>Найдите модель автомобиля по VIN номеру</p>

                            @using (Html.BeginForm("Index", "Home", FormMethod.Get))
                            {
                                <div class="search-box1 catalog_search">
                                    @Html.TextBox("Vin")
                                    @Html.DropDownList("SelectedAuto", new SelectList(ViewBag.UniqueAuto, "Value", "Text", "ViewBag.Auto"), "Выберите модель по параметрам")
                                    <input type="submit" name="SearchVin" value="Поиск" />
                                </div>
                            }


                         
                            <p>
                                VIN вашего автомобиля является самым надежным идентификатором.
                                Если Вы не знаете VIN или сомневаетесь, подойдет ли выбранная деталь
                                к вашему автомобилю - свяжитесь с Вашим менеджером
                            </p>
                        </td>
                        <td class="delimetercell2"></td>
                        <td style="width: 5%; text-align: center; position: relative;">
                            <div class="celldelimetercaption">ИЛИ</div>
                        </td>

                        <td style="padding-left: 20px; width: 50%">
                            @using (Html.BeginForm("Catalog", "Home", FormMethod.Get))
                            {
                                <p>Введите vin код</p>
                                @Html.TextBox("Vin")
                                <br>
                                <p>Выберите модель по параметрам</p>
                                @Html.DropDownList("SelectedAuto",
                                     new SelectList(ViewBag.UniqueAuto, "Value", "Text", "ViewBag.Auto"), "All", new { onchange = "form.submit();" })
                                <br>
                                <a href="@ViewBag.Url@ViewBag.Vin" class="btn" style="color:white">Поиск  </a>
                            }
                        </td>


                    </tr>
                    <tr>
                        <td>
                            <p>Найдите модель автомобиля по FRAME номеру</p>
                            <form action="/AdvancedSearch/ModificationsFrame" method="get" novalidate="novalidate">
                                <input id="frame" name="frame" placeholder="код кузова" style="border-radius: 4px;">
                                <input id="frameNo" name="frameNo" placeholder="номер кузова" style="border-radius: 4px;">
                                <div>например: ACR50-0195054</div>
                                <input id="advsearchbuttonmain" type="submit" value="Поиск">
                            </form>                <p>
                                Номер кузова — указывается в техпаспорте (СРТС) с обратной стороны документа, где нет печати, шестая строка сверху.
                                Состоит он из букв и цифр и обычно содержит около 10 знаков. На автомашине номер кузова обычно выбивается на металлической табличке под капотом.
                                При этом у разных марок авто эта табличка может располагаться в совершенно разных местах.
                                К примеру, у некоторых моделей Suzuki номер кузова выбит в колёсной арке над правым колесом.
                            </p>
                        </td>
                    </tr>
