
@{
    ViewBag.Title = "Cart";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Car.Controllers;
    

<div class="empty-box"></div>

<div class="context-box">
    <div class="container clearfix">
        <div class="left-column">
            <h1>@ViewBag.CartCart</h1>

        </div>
        <div class="right-column">
            <div class="login" style="margin-bottom: -200px;">




                <table class="basketstable basketCart">

                    <tbody>
                        <tr>
                            <td class="lastcat lastcatCart" colspan="11"><h3 style="margin-top: 10px;align-content: center;">@ViewBag.Message</h3></td>
                        </tr>
                    </tbody>
                </table>


            </div>




        </div>

    </div>
</div>
