
@model  List<Car.Controllers.ItemRemoteStore>
<div class="modal-content">
    <div class="login">

        <section class="content-wrapper main-content clear-fix">


            <div class="rounded">


                <h2>Изменение количества</h2>

            @using (Html.BeginForm("EditRS", "ShoppingCart", FormMethod.Post))
            {
                    <fieldset>
                        <div class="editor-label">
                            <label for="quant">Количество</label>
                        </div>
                        <div class="editor-field">
                           @Html.Editor("quant")
                        </div>
                        <p>
                          <input type="submit" value="Сохранить">
                       </p>
                    </fieldset>
                }
            </div>
            <div class="clear-fix"></div>

        </section>
    </div>
    </div>
















